{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "plugin:react/recommended",
    // "standard-with-typescript",
    "next",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "plugins": ["react"],
  "rules": { "@typescript-eslint/naming-convention": "off" },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
